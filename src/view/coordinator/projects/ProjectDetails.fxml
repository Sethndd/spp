<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" prefHeight="550.0" prefWidth="400.0">
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="80.0" prefWidth="400.0" text="Proyecto">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <VBox alignment="CENTER_LEFT" maxWidth="355.0" prefHeight="420.0" prefWidth="360.0" spacing="2.0">
            <Label text="Nombre: ">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblName" text=" "/>
            <Label text="Descripción:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblDescription" text=" "/>
            <Label text="Responsabilidades:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblResponsibilities" text=" "/>
            <Label text="Actividades:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblActivities" text=" "/>
            <Label text="Duración:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblDuration" text=" "/>
            <Label text="Objetivo General:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblObjective" text=" "/>
            <Label text="Metodología:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblMethodology" text=" "/>
            <Label text="Recursos:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblResource" text=" "/>
            <Label text="Responsable:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblManager" text=" "/>
            <Label text="Empresa:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="lblOrganization" text=" "/>
        </VBox>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0">
            <Button fx:id="btnBackward" onMouseClicked="#backwardByClick" onKeyPressed="#backwardByIntro" prefWidth="80.0" text="Regresar"/>
        </HBox>
    </VBox>
</AnchorPane>
