<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="470.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" prefHeight="470.0" prefWidth="400.0">
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="80.0" prefWidth="400.0" text="Editar organización">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <VBox alignment="CENTER_LEFT" maxWidth="355.0" prefHeight="370.0" prefWidth="346.0" spacing="2.0">
            <Label text="Nombre: ">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="txtName" />
            <Label text="Sector">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="txtSector" />
            <Label text="E-Mail">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="txtEMail" />
            <Label text="Numero telefónico:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="txtPhoneNumber" />
            <Label text="Estado">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <ComboBox fx:id="cboxState" prefWidth="360.0" />
            <Label text="Cuidad">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="txtCity" />
            <Label text="Dirección">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="txtAddress" />
        </VBox>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0">
            <Button fx:id="btnBackward" onKeyPressed="#backwardByIntro" onMouseClicked="#backwardByClick" prefWidth="80.0" text="Regresar" />
            <Button fx:id="btnSave" onKeyPressed="#saveByIntro" onMouseClicked="#saveByClick" prefWidth="80.0" text="Guardar" />
        </HBox>
    </VBox>
</AnchorPane>
