<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="430.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" prefHeight="430.0" prefWidth="400.0">
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="80.0" prefWidth="400.0" text="Proyecto">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <VBox alignment="CENTER_LEFT" maxWidth="355.0" prefHeight="300.0" prefWidth="355.0" spacing="2.0">
            <Label text="Nombre: ">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblName" text=" " />
            <Label text="Sector:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblSector" text=" " />
            <Label text="e-Mail:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblEMail" text=" " />
            <Label text="Numero telefónico:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblPhoneNumber" text=" " />
            <Label text="Estado:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblState" text=" " />
            <Label text="Cuidad:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblCity" text=" " />
            <Label text="Dirección:">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label fx:id="lblAddress" text=" " />
        </VBox>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0">
            <Button fx:id="btnBackward" onKeyPressed="#backwardByIntro" onMouseClicked="#backwardByClick" prefWidth="80.0" text="Regresar" />
        </HBox>
    </VBox>
</AnchorPane>
