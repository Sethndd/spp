<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" prefHeight="150.0" prefWidth="400.0">
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="80.0" prefWidth="400.0" text="¿Está seguro que desea eliiminar esta organización? Los cambios son irreversibles." textAlignment="CENTER" wrapText="true" />
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0">
            <Button fx:id="btnAccept" onMouseClicked="#acceptByClick" onKeyPressed="#acceptByIntro" prefWidth="80.0" text="Aceptar" />
            <Button fx:id="btnCancel" onMouseClicked="#cancelByClick" onKeyPressed="#cancelByIntro" prefWidth="80.0" text="Cancelar" />
        </HBox>
    </VBox>
</AnchorPane>
